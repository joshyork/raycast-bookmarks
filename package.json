{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bookmarks",
  "title": "Bookmarks",
  "description": "",
  "icon": "command-icon.png",
  "author": "JoshYork",
  "license": "MIT",
  "commands": [
    {
      "name": "create-bookmark",
      "title": "Create Bookmark",
      "description": "",
      "mode": "view"
    },
    {
      "name": "search-bookmarks",
      "title": "Search Bookmarks",
      "description": "",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "gh_access_token",
      "type": "textfield",
      "required": true,
      "title": "GitHub Personal Access token",
      "description": "https://docs.github.com/en/authentication/keeping-your-account-and-data-secure/creating-a-personal-access-token"
    },
    {
      "name": "gh_gist_id",
      "type": "textfield",
      "required": true,
      "title": "GitHub Gist Id",
      "description": "https://gist.github.com/"
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "script:fromFilemark": "ts-node ./scripts/fromFilemark.ts"
  },
  "dependencies": {
    "@octokit/types": "^7.3.1",
    "@raycast/api": "^1.30.3",
    "@typedash/typedash": "^0.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/uuid": "^8.3.4",
    "fs-extra": "^10.0.1",
    "fuse.js": "^6.5.3",
    "lowdb": "^3.0.0",
    "octokit": "^2.0.7",
    "uuid": "^8.3.2",
    "zod": "^3.19.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/ramda": "^0.28.14",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "ramda": "^0.28.0",
    "react-devtools": "^4.19.2",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.3"
  }
}
